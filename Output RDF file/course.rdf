    <rdfs:label xml:lang="en">Distributed Systems and Cloud Computing  - 2013Spring</rdfs:label>
    <REVE:hasConstituent rdf:resource="http://data.eurecom.fr/coursesession/32704"/>
    <REVE:hasConstituent rdf:resource="http://data.eurecom.fr/coursesession/32707"/>
